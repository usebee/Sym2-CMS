{% extends 'CMSAdminBundle::admin.html.twig' %}
{% block body %}
<h1>MediaCategory</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updated_at</th>
            <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <td colspan="2" width="100%">
                <a href="{{ path('admin_mediacategory', {'page' : 1}) }}" class="btn">Back To List</a>
                &nbsp;
                <a href="{{ path('admin_mediacategory_edit', { 'id': entity.id }) }}" class="btn">Edit</a>
            </td>
        </tr>
    </tbody>
</table>
{% endblock %}