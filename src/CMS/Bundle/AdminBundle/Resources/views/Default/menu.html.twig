{% for gkey, group in groups %}
    {% if group|length > 0 %}
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{ gkey }}<span class="caret"></span></a>
        <ul class="dropdown-menu">
            {% for item in group %}
            <li>
                <a href="{{ path(item.action[1]) }}">{{ item.label|trans({}, 'CMSAdminBundle') }}</a>
            </li>
            {% endfor %}
        </ul>
    </li>
    {% endif %}
{% endfor %}
