  <div class="masthead">
    <h2 class="muted">Logo Here</h3>        
    <div class="navbar">
      <div class="navbar-inner">
        <div class="container">
          <ul class="nav">
            {% for key, value in menus %}
                {% if active == key %}
                    <li class="active">
                {% else %}
                    <li>
                {% endif %}
                    <a href="{{ key }}">{{ value }}</a></li>                        
            {% endfor %}
          </ul>
        </div>
      </div>
    </div>
  </div>