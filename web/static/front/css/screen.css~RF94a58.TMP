	#header h1 {position:relative; top:0; left:0; z-index:9999}
	#nav {font-size:32px; text-transform:uppercase; line-height:32px; position: absolute; top:115px; z-index: 5; font-family:Gravur-CondensedBlack}
	#nav a:hover {text-decoration:none;}
		#nav > ul > li > a span {background:#000; line-height:30px; height:30px; float:left}
		#nav > ul > li > a:hover span {background:#ffffff; color: #000}
		#nav > ul > li.off > ul {display: none;}	
		#nav > ul > li.on > ul {display: block; width:100%; overflow:hidden}
		#nav li  {margin: 4px 0 0; overflow: hidden;}
		#nav > ul > li.on > ul li a {color: #b6b6b6; background-color:#fff; font-size:54px; line-height:48px}
		#nav > ul > li.on > ul li a:hover {color: #fff; background:#000;}


	#main {width: 100%; overflow: hidden;}
	#sidebar {position:absolute; right:0; top:27%; color:#fff; font-size:14px; font-family:Gravur-Condensed}
		.intro-text {width:120px; background:url(../images/bgd-repeat.png) repeat 0 0; padding:10px 0 10px 10px;
			opacity: 0.8;
			-moz-opacity: 0.8;
			filter:alpha(opacity=80);}
		#sidebar p {padding-right:2px; font-size:14px}
		#sidebar p a {color:#ff9000; text-transform:uppercase}
	#footer {width:100%; height:28px; line-height:28px; overflow:hidden; background:#000; color:#fff; font-size:10px; position:fixed; bottom:0; left:0; z-index:99}	
	#footer p {float:left; padding-left:15px; color:#fff}
