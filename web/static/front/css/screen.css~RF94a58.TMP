/* structure
-------------------------------------------------------*/
#container {width: 100%; height:100%; display:block; overflow: hidden; position: absolute; top: 0; background: #000; font-size:12px; font-family:Verdana, Geneva, sans-serif}
#container-outer {width:100%; overflow:hidden; margin:0 auto; min-height: 100%; height: auto !important; height: 100%; font-size:13px; line-height:16px; position:relative}
#container-inner {max-width:765px; min-width:615px; /*margin:28px auto 0;*/ margin:0 auto}
